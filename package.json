{
  "name": "@wan54/node-printer",
  "description": "Node.js printer bindings",
  "version": "0.6.10",
  "homepage": "https://github.com/wan54/node-printer",
  "author": {
    "name": "Ion Lupascu",
    "url": "http://program-support.co.uk/",
    "email": "ionlupascu@gmail.com"
  },
  "contributors": [
    "wan",
    "Timo Kunze <tkunze@compeso.com",
    "Thiago Lugli <thiagoelg@gmail.com>",
    "Eko Eryanto <ekoeryanto@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/wan54/node-printer.git"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "apply-patches": "patch-package",
    "prebuild-all": "prebuild --all --force --strip --verbose --openssl_fips=X",
    "prebuild": "prebuild",
    "rebuild": "node-gyp rebuild"
  },
  "binary": {
    "module_name": "node_printer",
    "module_path": "./lib/",
    "host": "https://github.com/wan54/node-printer/releases/download/",
    "remote_path": "v{version}"
  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "main": "./lib/printer",
  "types": "types/index.d.ts",
  "devDependencies": {
    "node-gyp": "^10.2.0",
    "prebuild": "^13.0.1",
    "node-abi": "^3.71.0",
    "patch-package": "^8.0.0"
  },
  "dependencies": {
    "nan": "^2.22.0",
    "prebuild-install": "^7.1.2"
  }
}
